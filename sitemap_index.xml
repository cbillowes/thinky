---
layout: null
sitemap: no
---
<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/sitemap/style.xsl"?>
{% assign url = "/sitemap" | prepend: site.baseurl | prepend: site.url %}
{% assign timestamp = site.time | date_to_xmlschema %}
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <sitemap>
    <loc>{{ url }}/pages.xml</loc>
    <lastmod>{{ timestamp }}</lastmod>
		<changefreq>monthly</changefreq>
		<priority>0.5</priority>
  </sitemap>
  <sitemap>
    <loc>{{ url }}/blog.xml</loc>
    <lastmod>{{ timestamp }}</lastmod>
		<changefreq>weekly</changefreq>
		<priority>1</priority>
  </sitemap>
  <sitemap>
    <loc>{{ url }}/code.xml</loc>
    <lastmod>{{ timestamp }}</lastmod>
		<changefreq>weekly</changefreq>
		<priority>1</priority>
  </sitemap>
</sitemapindex>
{% assign url = nil %}
{% assign timestamp = nil %}
<!-- XML Sitemap generated by Jekyll v{{ jekyll.version }}  -->
